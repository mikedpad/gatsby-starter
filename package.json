{
  "name": "gatsby-starter",
  "version": "0.6.5",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "description": "A Gatsby starter kit point for projects",
  "homepage": "https://mikedpad.github.io/gatsby-starter/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/gatsby-starter.git"
  },
  "scripts": {
    "clean": "rm -rf .cache",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "format": "prettier --check 'src/**/*.{js,jsx}'",
    "start": "npm run develop",
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "polished": "^4.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.3",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^3.3.0",
    "gatsby-plugin-image": "^1.3.1",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-offline": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.0",
    "gatsby-plugin-styled-components": "^4.3.0",
    "gatsby-plugin-webfonts": "^2.1.0",
    "gatsby-remark-images": "^5.0.0",
    "gatsby-remark-unwrap-images": "^1.0.2",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-json": "^3.3.0",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "gh-pages": "^3.1.0",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "slugify": "^1.5.0"
  }
}

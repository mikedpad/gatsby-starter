{
  "name": "gatsby-starter",
  "version": "0.6.0",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "description": "A Gatsby starter kit point for projects",
  "homepage": "https://mikedpad.github.io/gatsby-starter/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/gatsby-starter.git"
  },
  "scripts": {
    "clean": "rm -rf .cache",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "start": "npm run develop",
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby": "^2.24.51",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-manifest": "^2.4.25",
    "gatsby-plugin-offline": "^3.2.25",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.29",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-webfonts": "^1.1.3",
    "gatsby-remark-images": "^3.3.27",
    "gatsby-remark-unwrap-images": "^1.0.2",
    "gatsby-source-filesystem": "^2.3.26",
    "gatsby-transformer-json": "^2.4.11",
    "gatsby-transformer-remark": "^2.8.30",
    "gatsby-transformer-sharp": "^2.5.13",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.1",
    "slugify": "^1.4.5"
  }
}

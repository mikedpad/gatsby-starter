{
  "name": "gatsby-starter",
  "version": "0.6.3",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "description": "A Gatsby starter kit point for projects",
  "homepage": "https://mikedpad.github.io/gatsby-starter/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/gatsby-starter.git"
  },
  "scripts": {
    "clean": "rm -rf .cache",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "format": "prettier --check 'src/**/*.{js,jsx}'",
    "start": "npm run develop",
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "polished": "^4.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-offline": "^3.3.3",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-webfonts": "^1.1.3",
    "gatsby-remark-images": "^3.4.2",
    "gatsby-remark-unwrap-images": "^1.0.2",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-transformer-json": "^2.4.15",
    "gatsby-transformer-remark": "^2.9.2",
    "gatsby-transformer-sharp": "^2.5.21",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.2",
    "slugify": "^1.4.6"
  }
}
